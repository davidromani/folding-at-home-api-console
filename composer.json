{
    "name": "davidromani/folding-at-home-api-console",
    "description": "A command line tool to interact with Folding@Home API project",
    "type": "library",
    "keywords": ["symfony", "console", "foldingathome", "php7", "command"],
    "homepage": "https://www.flux.cat/en/home",
    "license": "MIT",
    "authors": [
        {
            "name": "David Roman√≠",
            "email": "david@flux.cat",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "beberlei/doctrineextensions": "^1.2",
        "doctrine/orm": "^2.7",
        "symfony/console": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/http-foundation": "4.4.*",
        "symfony/property-access": "4.4.*",
        "symfony/serializer": "4.4.*"
    },
    "require-dev": {},
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0-dev"
        }
    }
}
